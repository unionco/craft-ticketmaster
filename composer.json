{
    "name": "unionco/craft-ticketmaster",
    "description": "Ticket master ticket feed for venues.",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "ticketmaster"
    ],
    "support": {
        "docs": "https://github.com/unionco/craft-ticketmaster/blob/master/README.md",
        "issues": "https://github.com/unionco/craft-ticketmaster/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Union",
            "homepage": "https://github.com/unionco"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0",
        "adbario/php-dot-notation": "^2.2"
    },
    "autoload": {
        "psr-4": {
          "unionco\\ticketmaster\\": "src/"
        }
    },
    "extra": {
        "name": "Ticketmaster",
        "handle": "ticketmaster",
        "hasCpSettings": true,
        "changelogUrl": "https://raw.githubusercontent.com/unionco/craft-ticketmaster/master/CHANGELOG.md",
        "class": "unionco\\ticketmaster\\Ticketmaster"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
